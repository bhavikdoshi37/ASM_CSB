<core:FragmentDefinition xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:core="sap.ui.core"
	xmlns:custom="colgate.asm.planning.base.util" xmlns:f="sap.ui.layout.form" xmlns="sap.m" xmlns:uc="sap.ui.unified.calendar">
	<Dialog title="">
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="SimpleFormDisplay354" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" title=""
				labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
				<f:content>
					<Label text="{i18n&gt;CP_MonthSelLbl}"/>
					<uc:MonthPicker id="idMonthPicker" select="onDateDialogConfirm" month="{Copy>/MonthSelect}"></uc:MonthPicker>
					<Label text="{i18n&gt;CP_MonthYrLbl}"/>
				</f:content>
			</f:SimpleForm>
		</VBox>
		<buttons>
			<Button text="{i18n>AC_cancel}" press="onDateDialogClose"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>